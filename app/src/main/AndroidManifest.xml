<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.khalid.health">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.almissbah.health.ui.MainActivity"
            android:supportsRtl="false"
            android:theme="@style/AppTheme.NoActionBar" >

        </activity>
        <activity android:name="com.almissbah.health.ui.AddUserActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.almissbah.health.ui.PickAgeActivity"
            android:label="@string/title_activity_pick_age"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.almissbah.health.ui.SplashActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="com.almissbah.health.MyNotificationPublisher" >
        </receiver>
    </application>

</manifest>